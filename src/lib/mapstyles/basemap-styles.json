	{
		"id": "water",
		"type": "fill",
		"source": "vector-tiles",
		"source-layer": "water",
		"filter": [
		"all",
		["==", "$type", "Polygon"],
		["!=", "intermittent", 1],
		["!=", "brunnel", "tunnel"]
		],
		"layout": {"visibility": "visible"},
		"paint": {
			"fill-color": {"stops": [[0, "#ffffff"], [8, "#ffffff"], [8.1, "#cde2ec"],[22, "#cde2ec"]]}
		}
	},
	{
		"id": "water_intermittent",
		"type": "fill",
		"source": "vector-tiles",
		"source-layer": "water",
		"filter": ["all", ["==", "$type", "Polygon"], ["==", "intermittent", 1]],
		"layout": {"visibility": "visible"},
		"paint": {
			"fill-color": {"stops": [[0, "#ffffff"], [8, "#ffffff"], [8.1, "#cde2ec"],[22, "#cde2ec"]]}
		}
	},
	{
		"id": "waterway-tunnel",
		"type": "line",
		"source": "vector-tiles",
		"source-layer": "waterway",
		"filter": [
		"all",
		["==", "$type", "LineString"],
		["==", "brunnel", "tunnel"]
		],
		"layout": {"visibility": "visible"},
		"paint": {
			"line-color": "#cde2ec",
			"line-dasharray": [3, 3],
			"line-gap-width": {"stops": [[12, 0], [20, 6]]},
			"line-opacity": 1,
			"line-width": {"base": 1.4, "stops": [[8, 1], [20, 2]]}
		}
	},
	{
		"id": "waterway",
		"type": "line",
		"source": "vector-tiles",
		"source-layer": "waterway",
		"filter": [
		"all",
		["==", "$type", "LineString"],
		["!in", "brunnel", "tunnel", "bridge"],
		["!=", "intermittent", 1]
		],
		"layout": {"visibility": "visible"},
		"paint": {
			"line-color": {"stops": [[0, "#ffffff"], [8, "#ffffff"], [8.1, "#cde2ec"],[22, "#cde2ec"]]},
			"line-opacity": 1,
			"line-width": {"base": 1.4, "stops": [[8, 1], [20, 8]]}
		}
